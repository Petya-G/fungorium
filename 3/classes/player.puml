@startuml player
abstract class Player{
    id : Integer
}

Player <|-- Mushroomer
Mushroomer o-- "0..*" Spores
Mushroomer o-- "1..*" Body
Mushroomer o-- "0..*" Thread
class Mushroomer{
    colonies : Colony[]
    plantBody(Tecton) : void
    throwSpore(Tecton, Spores) : void
}

Player <-- Insecter
Insecter o-- Insect
class Insecter{
    moveInsect(Insect) : void
    cutThread(Insect) : void
}
@enduml